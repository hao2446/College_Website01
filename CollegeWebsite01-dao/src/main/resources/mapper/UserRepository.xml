<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.swpu.info.college_website.dao.Impl.UserRepositoryImpl">
    <select id="selectByUserName" parameterType="java.lang.String" resultType="cn.edu.swpu.info.admin">
        select * from  admin where loginName=#{loginName}
    </select>

    <select id="selectByUserId" parameterType="java.lang.Integer" resultType="cn.edu.swpu.info.admin">
        select * from admin where adminId=#{adminId}
    </select>
</mapper>