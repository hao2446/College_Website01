<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.swpu.info.college_website.dao.Impl.PermissionsDaoImpl">
    <!--登录验证时，需要根据用户名对密码进行查询-->
    <select id="selectSomeInfo" parameterType="java.lang.String" resultType="cn.edu.swpu.info.admin">
        select loginName,adminPassword,adminPermissions from admin where loginName=#{loginName}
    </select>
</mapper>